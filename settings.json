    "vim.useSystemClipboard": true,
    "vim.normalModeKeyBindingsNonRecursive": [
        // Git
        {
            "before": [
                "leader",
                "g"
            ],
            "commands": [
                "lazygit.openLazygit"
            ]
        },
        {
            "before": [
                "leader",
                "s",
                "d"
            ],
            "commands": [
                "workbench.action.debug.start"
            ],
            "when": [
                "debuggersAvailable && debugState == 'inactive'"
            ]
        },
        {
            "before": [
                "leader",
                "s",
                "d",
                "c"
            ],
            "commands": [
                "workbench.action.debug.stop"
            ],
            "when": [
                "inDebugMode && !focusedSessionIsAttach"
            ]
        },
        {
            "before": [
                "leader",
                "s",
                "d",
                "r"
            ],
            "commands": [
                "workbench.action.debug.restart"
            ],
            "when": [
                "inDebugMode"
            ]
        },
        {
            "before": [
                "leader",
                "b",
            ],
            "commands": [
                "editor.debug.action.toggleBreakpoint"
            ],
            "when": [
                "debuggersAvailable && disassemblyViewFocus || debuggersAvailable && editorTextFocus"
            ]
        },
        // NAVIGATION
        {
            "before": [
                ","
            ],
            "after": [
                "shift",
                "'"
            ]
        },
        {
            "before": [
                "g",
                "i"
            ],
            "commands": [
                "editor.action.goToImplementation"
            ],
            "when": [
                "editorHasImplementationProvider && editorTextFocus"
            ],
        },
        // switch b/w buffers
        {
            "before": [
                "<S-h>"
            ],
            "commands": [
                ":bprevious"
            ]
        },
        {
            "before": [
                "<S-l>"
            ],
            "commands": [
                ":bnext"
            ]
        },
        // splits
        {
            "before": [
                "leader",
                "v"
            ],
            "commands": [
                ":vsplit"
            ]
        },
        {
            "before": [
                "leader",
                "s"
            ],
            "commands": [
                ":split"
            ]
        },
        // panes
        {
            "before": [
                "<C-h>"
            ],
            "commands": [
                "workbench.action.focusLeftGroup"
            ]
        },
        {
            "before": [
                "<C-j>"
            ],
            "commands": [
                "workbench.action.focusBelowGroup"
            ]
        },
        {
            "before": [
                "<C-k>"
            ],
            "commands": [
                "workbench.action.focusAboveGroup"
            ]
        },
        {
            "before": [
                "<C-l>"
            ],
            "commands": [
                "workbench.action.focusRightGroup"
            ]
        },
        // NICE TO HAVE
        {
            "before": [
                "p"
            ],
            "after": [
                "<S-p>"
            ]
        },
        {
            "before": [
                "leader",
                "d"
            ],
            "after": [
                "d",
                "d"
            ]
        },
        {
            "before": [
                "leader",
                "w"
            ],
            "commands": [
                ":w!"
            ]
        },
        {
            "before": [
                "leader",
                "x"
            ],
            "commands": [
                ":x!"
            ]
        },
        {
            "before": [
                "<leader>",
                "q",
                "a"
            ],
            "commands": [
                "openEditors.closeAll"
            ]
        },
        {
            "before": [
                "<leader>",
                "q",
                "o"
            ],
            "commands": [
                "workbench.action.closeOtherEditors"
            ]
        },
        {
            "before": [
                "8",
                "d"
            ],
            "commands": [
                "editor.action.marker.prev"
            ]
        },
        {
            "before": [
                "9",
                "d"
            ],
            "commands": [
                "editor.action.marker.next"
            ]
        },
        {
            "before": [
                "<leader>",
                "c",
                "a"
            ],
            "commands": [
                "editor.action.quickFix"
            ]
        },
        {
            "before": [
                "leader",
                "f",
                "m"
            ],
            "commands": [
                "editor.action.formatDocument"
            ]
        },
        {
            "before": [
                "leader",
                "f",
                "l"
            ],
            "commands": [
                "editor.toggleFold"
            ]
        },
        {
            "before": [
                "g",
                "h"
            ],
            "commands": [
                "editor.action.showDefinitionPreviewHover"
            ]
        },
        {
            "before": [
                "<c-a>"
            ],
            "after": [
                "g",
                "g",
                "V",
                "G"
            ]
        },
        {
            "before": [
                "<s-k>"
            ],
            "commands": [
                "editor.action.showHover"
            ]
        },
        {
            "before": [
                "<leader>",
                "."
            ],
            "commands": [
                ":norm A.<CR>"
            ]
        },
        {
            "before": [
                "<leader>",
                ","
            ],
            "commands": [
                ":s/\\v\\s*(,\\s*)*$/,/<CR>:nohl<CR>"
            ]
        },
        {
            "before": [
                "<leader>",
                ";"
            ],
            "commands": [
                // ":s/\\v\\s*(;\\s*)*$/;/<CR>:nohl<CR>"
                ":let @z = @0 | normal! mZ$A; | normal! `Z"
            ]
        },
    ],
    "vim.visualModeKeyBindings": [
        // Stay in visual mode while indenting
        {
            "before": [
                "<"
            ],
            "commands": [
                "editor.action.outdentLines"
            ]
        },
        {
            "before": [
                ">"
            ],
            "commands": [
                "editor.action.indentLines"
            ]
        },
        // Move selected lines while staying in visual mode
        {
            "before": [
                "J"
            ],
            "commands": [
                "editor.action.moveLinesDownAction"
            ]
        },
        {
            "before": [
                "K"
            ],
            "commands": [
                "editor.action.moveLinesUpAction"
            ]
        },
        // toggle comment selection
        {
            "before": [
                "leader",
                "c"
            ],
            "commands": [
                "editor.action.commentLine"
            ]
        },
        {
            "before": [
                "leader",
                "s",
                "l"
            ],
            "commands": [
                "sortLines.sortLines"
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "p"
            ],
            "after": [
                "P"
            ]
        }
    ],
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "C-h",
            ],
            "commands": [
                "cursorLeft"
            ],
            "when": "textInputFocus"
        },
        {
            "before": [
                "C-l",
            ],
            "commands": [
                "cursorRight"
            ],
            "when": "textInputFocus"
        },
        {
            "before": [
                "j",
                "j",
            ],
            "after": [
                "<Esc>"
            ],
        },
    ],
